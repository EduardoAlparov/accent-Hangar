{{#extend "layout"}}
{{#content "main"}}

<section class="image-intro image-intro--no-height">
  <div class="site-container">
    {{>breadcrumbs}}
  </div>
</section>

<section class="reviews-list">

  <div class="site-container">
    <h1 class="reviews-list__title text-h2">
      Наши объекты
