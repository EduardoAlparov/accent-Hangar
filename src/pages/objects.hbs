    {{#if tabsList}}
    <div class="prices-navigation">
      <ul class="prices-navigation__list hide-on-tablet">
        {{#each tabsList}}
        <li class="prices-navigation__item">
          <button type="button" class="prices-navigation__tab tab js-prices-tab" data-btn={{this.data}}>
            {{this.text}}
          </button>
        </li>
        {{/each}}
      </ul>

      <div class="prices-navigation__mobile-button show-on-tablet">
        <button type="button" class="icon-tab" data-path="modal-tabs">
          <span class="js-price-type-value">Все категории</span>
          <i></i>
        </button>
      </div>
    </div>
    {{/if}}

